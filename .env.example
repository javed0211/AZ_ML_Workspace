# Azure Configuration
AZURE_TENANT_ID=your-tenant-id-here
AZURE_SUBSCRIPTION_ID=your-subscription-id-here
AZURE_RESOURCE_GROUP=your-resource-group-name
AZURE_WORKSPACE_NAME=your-workspace-name

# Authentication (optional - defaults to DefaultAzureCredential)
USE_INTERACTIVE_AUTH=false
USE_MANAGED_IDENTITY=false

# URLs (optional - will be auto-generated if not provided)
BASE_URL=https://ml.azure.com
VSCODE_WEB_URL=https://your-workspace.vscode.azure.com
JUPYTER_URL=https://your-workspace.jupyter.azure.com

# Timeouts (optional)
DEFAULT_TIMEOUT=30000
NAVIGATION_TIMEOUT=60000
TEST_TIMEOUT=300000

# Retry Configuration (optional)
MAX_RETRIES=3

# Parallel Execution (optional)
MAX_WORKERS=4

# Artifacts (optional)
UPLOAD_ARTIFACTS=false

# Logging (optional)
LOG_LEVEL=info
LOG_FORMAT=json

# Electron/VSCode Path (optional)
VSCODE_PATH=/Applications/Visual Studio Code.app/Contents/MacOS/Electron

# Compute Configuration (optional)
COMPUTE_INSTANCE_NAME=your-compute-instance
COMPUTE_SIZE=Standard_DS3_v2

# PIM Configuration (optional)
PIM_ROLE_NAME=your-pim-role
PIM_JUSTIFICATION=Automated testing

# Notifications (optional)
TEAMS_WEBHOOK_URL=https://your-org.webhook.office.com/webhookb2/...
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/...
EMAIL_NOTIFICATIONS=false

# Environment
NODE_ENV=development
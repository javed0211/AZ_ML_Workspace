# Azure Configuration
AZURE_TENANT_ID=your-tenant-id
AZURE_CLIENT_ID=your-client-id
AZURE_CLIENT_SECRET=your-client-secret
AZURE_SUBSCRIPTION_ID=your-subscription-id
AZURE_RESOURCE_GROUP=your-resource-group
AZURE_ML_WORKSPACE_NAME=your-workspace-name
AZURE_KEY_VAULT_URL=https://your-keyvault.vault.azure.net/

# Authentication
USE_INTERACTIVE_AUTH=false
USE_MANAGED_IDENTITY=false

# Test Configuration
BASE_URL=https://ml.azure.com
VSCODE_WEB_URL=https://your-workspace.vscode.azure.com
JUPYTER_URL=https://your-workspace.jupyter.azure.com

# Timeouts (in milliseconds)
DEFAULT_TIMEOUT=30000
NAVIGATION_TIMEOUT=60000
TEST_TIMEOUT=300000

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY=1000

# Parallel Execution
MAX_WORKERS=4

# Artifacts
ARTIFACTS_PATH=./test-results
UPLOAD_ARTIFACTS=false
AZURE_STORAGE_ACCOUNT=your-storage-account
AZURE_STORAGE_CONTAINER=test-artifacts

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Electron (VS Code Desktop)
VSCODE_PATH=/Applications/Visual Studio Code.app/Contents/MacOS/Electron
ELECTRON_USER_DATA_DIR=./.vscode-test

# Compute
COMPUTE_INSTANCE_NAME=test-compute-instance
COMPUTE_CLUSTER_NAME=test-compute-cluster
COMPUTE_SIZE=Standard_DS3_v2

# PIM (Privileged Identity Management)
PIM_ROLE_NAME=Contributor
PIM_SCOPE=/subscriptions/your-subscription-id/resourceGroups/your-resource-group
PIM_JUSTIFICATION=Automated testing

# Notifications
TEAMS_WEBHOOK_URL=https://your-org.webhook.office.com/webhookb2/...
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/...
EMAIL_NOTIFICATIONS=false

# Environment
NODE_ENV=development
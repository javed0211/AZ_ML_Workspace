{
  "subscription_id": "your-subscription-id",
  "resource_group": "your-resource-group",
  "workspace_name": "your-workspace-name",
  "location": "eastus",
  "compute_targets": {
    "cpu_cluster": {
      "name": "cpu-cluster",
      "vm_size": "STANDARD_DS3_V2",
      "min_nodes": 0,
      "max_nodes": 4
    },
    "gpu_cluster": {
      "name": "gpu-cluster", 
      "vm_size": "STANDARD_NC6",
      "min_nodes": 0,
      "max_nodes": 2
    }
  },
  "datastores": {
    "default": {
      "name": "workspaceblobstore",
      "type": "AzureBlob"
    }
  },
  "environments": {
    "python_env": {
      "name": "python-env",
      "python_version": "3.8",
      "packages": [
        "pandas",
        "numpy",
        "scikit-learn",
        "azureml-sdk"
      ]
    }
  }
}
{
  "$schema": "https://schemas.reqnroll.net/reqnroll-config-latest.json",
  "language": {
    "feature": "en-US"
  },
  "bindingCulture": {
    "name": "en-US"
  },
  "runtime": {
    "dependencies": [
      {
        "type": "plugin",
        "name": "Reqnroll.NUnit"
      }
    ],
    "stopAtFirstError": false,
    "missingOrPendingStepsOutcome": "Inconclusive",
    "obsoleteBehavior": "Warn"
  },
  "generator": {
    "allowDebugGeneratedFiles": false,
    "allowRowTests": true,
    "generateAsyncTests": true,
    "addNonParallelizableMarkerForTags": [
      "serial",
      "no-parallel"
    ],
    "markFeaturesParallelizable": true,
    "skipParallelizableMarkerForTags": [
      "serial",
      "no-parallel",
      "ui"
    ]
  },
  "trace": {
    "traceSuccessfulSteps": true,
    "traceTimings": true,
    "minTracedDuration": "0:0:0.1",
    "stepDefinitionSkeletonStyle": "RegexAttribute",
    "coloredOutput": true
  },
  "stepAssemblies": [],
  "bindingAssemblies": [],
  "cucumber": {
    "enabled": true
  }
}
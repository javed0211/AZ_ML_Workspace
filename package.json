{
  "name": "azure-ml-workspace-automation",
  "version": "1.0.0",
  "description": "Azure ML Workspace Automation Framework using Playwright + TypeScript",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:smoke": "playwright test --grep @smoke",
    "test:electron": "playwright test --grep @electron",
    "test:notebook": "playwright test --grep @notebook",
    "test:pim": "playwright test --grep @pim",
    "test:compute": "playwright test --grep @compute",
    "test:website": "playwright test --grep @website",
    "test:roles": "node scripts/run-role-tests.js",
    "test:roles:owner": "node scripts/run-role-tests.js --role owner",
    "test:roles:contributor": "node scripts/run-role-tests.js --role contributor",
    "test:roles:reader": "node scripts/run-role-tests.js --role reader",
    "test:roles:data-scientist": "node scripts/run-role-tests.js --role dataScientist",
    "test:roles:compute-operator": "node scripts/run-role-tests.js --role computeOperator",
    "test:roles:pim": "node scripts/run-role-tests.js --role pim",
    "test:roles:all": "node scripts/run-role-tests.js --all",
    "test:roles:parallel": "node scripts/run-role-tests.js --all --parallel",
    "test:parallel": "playwright test --workers=4",
    "test:report": "playwright show-report",
    "test:allure": "allure serve allure-results",
    "test:vscode": "playwright test --grep \"@electron\"",
    "test:vscode:mock": "NODE_ENV=test MOCK_VSCODE=true MOCK_AZURE_SERVICES=true SKIP_AZURE_AUTH=true AZURE_TENANT_ID=test-tenant-id AZURE_CLIENT_ID=test-client-id AZURE_CLIENT_SECRET=test-client-secret AZURE_SUBSCRIPTION_ID=test-subscription-id AZURE_RESOURCE_GROUP=test-resource-group AZURE_ML_WORKSPACE_NAME=test-workspace playwright test --grep \"@electron.*@integration\" --reporter=line --workers=1",
    "test:vscode:integration": "playwright test --grep \"@electron.*@integration\"",
    "test:vscode:notebook": "playwright test --grep \"@electron.*@notebook\"",
    "test:vscode:remote": "playwright test --grep \"@electron.*@remote\"",
    "test:vscode:debug": "NODE_ENV=test MOCK_VSCODE=true MOCK_AZURE_SERVICES=true playwright test --grep \"@electron\" --debug",
    "example": "node examples/example-usage.js",
    "example:demo": "node examples/example-usage.js --demo",
    "example:test": "node examples/example-usage.js --test",
    "validate:testdata": "node scripts/validate-test-data.js",
    "create:testdata": "node scripts/create-test-data.js",
    "scaffold": "node scripts/scaffold-test.js",
    "setup": "node scripts/setup.js",
    "install:browsers": "playwright install",
    "install:deps": "playwright install-deps",
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist test-results playwright-report allure-results",
    "ui:dev": "cd ui && npm run dev",
    "ui:build": "cd ui && npm run build",
    "ui:start": "cd ui && npm start"
  },
  "keywords": [
    "azure",
    "ml",
    "workspace",
    "automation",
    "playwright",
    "typescript",
    "testing",
    "e2e"
  ],
  "author": "Zencoder",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "allure-playwright": "^2.10.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@azure/arm-compute": "^21.0.0",
    "@azure/arm-machinelearning": "^2.0.0",
    "@azure/arm-resources": "^5.0.0",
    "@azure/identity": "^4.0.0",
    "@azure/keyvault-secrets": "^4.0.0",
    "@azure/storage-blob": "^12.0.0",
    "@azure/msal-node": "^2.0.0",
    "@azure/ms-rest-nodeauth": "^3.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.0.0",
    "winston": "^3.11.0",
    "uuid": "^9.0.0",
    "joi": "^17.11.0",
    "cross-spawn": "^7.0.0",
    "tree-kill": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/azure-ml-workspace-automation.git"
  }
}